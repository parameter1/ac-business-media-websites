{
  "taglib-imports": [
    "./company-details/marko.json",
    "./contact-details/marko.json"
  ],
  "<global-content-default-layout>": {
    "template": "./default.marko",
    "<after-body>": {},
    "@sections <section>[]": {},
    "@id": "number",
    "@type": "string",
    "@page-node": "object",
    "@with-ad-injection": "boolean",
    "@with-ads": "boolean",
    "@with-bottom-ad-block": "boolean",
    "@with-comments": "boolean",
    "@with-company": "boolean",
    "@with-load-more": "boolean",
    "@with-newsletter-signup": "boolean",
    "@with-primary-image": "boolean",
    "@with-published-date": "boolean",
    "@with-recommended": "boolean",
    "@with-related-stories": "boolean",
    "@with-social-share": "boolean"
  },
  "<global-content-company-layout>": {
    "template": "./company.marko",
    "<after-body>": {},
    "<right-rail>": {},
    "@sections <section>[]": {},
    "@id": "number",
    "@top-company-data": "object",
    "@type": "string",
    "@page-node": "object"
  },
  "<global-content-company-details>": {
    "template": "./company-details.marko",
    "@obj": "object",
    "@show-email": {
      "type": "boolean",
      "default-value": true
    },
    "@tag": {
      "type": "string",
      "default-value": "div"
    },
    "@class": "string",
    "@modifiers": "array",
    "@attrs": "object"
  },
  "<global-content-contact-layout>": {
    "template": "./contact.marko",
    "@id": "number",
    "@type": "string",
    "@page-node": "object"
  },
  "<global-content-product-layout>": {
    "template": "./product.marko",
    "<after-body>": {},
    "<right-rail>": {},
    "@sections <section>[]": {},
    "@id": "number",
    "@type": "string",
    "@page-node": "object"
  },
  "<global-content-sponsored-layout>": {
    "template": "./sponsored.marko"
  },
  "<global-content-webinar-layout>": {
    "template": "./webinar.marko",
    "@id": "number",
    "@type": "string",
    "@page-node": "object"
  },
  "<global-content-whitepaper-layout>": {
    "template": "./whitepaper.marko",
    "@id": "number",
    "@type": "string",
    "@page-node": "object"
  },
  "<global-content-wrapper-layout>": {
    "template": "./wrapper.marko",
    "@sections <section>[]": {},
    "@scroll-depth-selector": "string",
    "@id": "number",
    "@type": "string",
    "@page-node": "object",
    "@load-more": "boolean",
    "@with-ads": "boolean"
  }
}
