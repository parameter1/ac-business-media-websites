{
  "taglib-imports": [
    "./blocks/marko.json",
    "./flows/marko.json",
    "./layouts/marko.json",
    "./leaders/marko.json",
    "./nodes/marko.json",
    "./parse.ly/marko.json"
  ],
  "<refresh-theme-native-x-inject>": {
    "template": "./native-x-inject.marko",
    "@limit": "number",
    "@indexes": "array",
    "@index": "number",
    "@name": "string",
    "@section-name": "string",
    "@aliases": "array",
    "@nodes": "array",
    "@config": "object"
  },
  "<refresh-theme-document>": {
    "template": "./document.marko",
    "<head>": {},
    "<above-container>": {},
    "<container>": {
      "<above-page>": {},
      "<page>": {},
      "<below-page>": {}
    },
    "<below-container>": {},
    "<foot>": {}
  },
  "<refresh-theme-fixed-ad-bottom>": {
    "template": "./fixed-ad-bottom.marko",
    "@aliases": "array"
  },
  "<refresh-theme-gam-media-fuse>": {
    "template": "./gam-media-fuse.marko",
    "@id" : "number"
  },
  "<refresh-theme-contextual-header>": {
    "template": "./site-header/contextual.marko"
  },
  "<refresh-theme-content-meter-block>": {
    "template": "./blocks/content-meter.marko"
  },
  "<refresh-theme-content-section-logo>": {
    "template": "./content-section-logo.marko",
    "<section>": {
      "@name": "string",
      "@hierarchy": "array"
    },
    "<image>": {}
  },
  "<refresh-theme-menu-toggle-button>": {
    "template": "./menu-toggle-button.marko"
  }
}
